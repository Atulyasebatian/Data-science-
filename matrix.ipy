import numpy as np

a=np.array([[1,2],[3,4]])
b=np.array([[5,6],[7,8]])

add=a+b
sub=a-b
mul=a*b
trans=a.T
dot=a@b

print("Matrix a:\n", a)
print("Matrix b:\n", b)

print("\nAddition:\n",add)
print("Subtraction:\n",sub)
print("Multiplication:\n",mul)
print("Transpose:\n",trans)
print("Dot product:\n",dot)

